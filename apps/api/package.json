{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run build-ts && npm run watch",
    "start": "npm run serve",
    "build": "npm run build-ts && npm run lint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc -b",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "clean": "rimraf ./dist"
  },
  "devDependencies": {
    "@avila-tek/tsconfig": "*",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/http-errors": "^2.0.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.15",
    "@types/node-cron": "^3.0.6",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.29.0",
    "eslint-config-avilatek": "^1.13.0",
    "eslint-config-avilatek-typescript": "^1.13.0",
    "eslint-config-custom": "*",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.11",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@apollo/utils.keyvaluecache": "^2.1.0",
    "@sentry/integrations": "^7.26.0",
    "@sentry/node": "^7.26.0",
    "@sentry/tracing": "^7.26.0",
    "argon2": "^0.30.2",
    "aws-sdk": "^2.1274.0",
    "axios": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "browser-detect": "^0.2.28",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "graphql": "^16.6.0",
    "graphql-compose": "^9.0.10",
    "graphql-compose-connection": "^8.2.1",
    "graphql-compose-mongoose": "^9.7.2",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^8.3.14",
    "helmet": "^6.0.1",
    "http-errors": "^2.0.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.8.0",
    "mongoose-algolia": "^1.10.1",
    "mongoose-to-swagger": "^1.4.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "node-cron": "^3.0.2",
    "postmark": "^3.0.15",
    "sanitize-html": "^2.8.0",
    "slugs": "^0.1.3",
    "swagger-routes-express": "^3.3.2",
    "tslib": "^2.4.1",
    "uuid": "^9.0.0",
