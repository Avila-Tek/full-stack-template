{
  "name": "@avila-tek/backend",
  "version": "1.1.0",
  "description": "...",
  "scripts": {
    "dev": "npm run build-ts && npm run watch",
    "start": "npm run serve",
    "build": "npm run build-ts && npm run lint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc -b",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "repository": {
    "type": "git",
    "url": "none"
  },
  "keywords": [
    "typescript",
    "express",
    "graphql",
    "apollo"
  ],
  "author": "Jose Roberto Quevedo <jose@avilatek.dev>",
  "license": "MIT",
  "dependencies": {
    "@sentry/integrations": "^6.8.0",
    "@sentry/node": "^6.8.0",
    "@sentry/tracing": "^6.8.0",
    "apollo-server": "^2.25.2",
    "apollo-server-errors": "^2.5.0",
    "apollo-server-express": "^2.25.2",
    "apollo-server-plugin-base": "^0.13.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "browser-detect": "^0.2.28",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dayjs": "^1.10.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "graphql": "^15.5.1",
    "graphql-compose": "^9.0.1",
    "graphql-compose-connection": "^8.2.0",
    "graphql-compose-mongoose": "^9.4.2",
    "graphql-middleware": "^6.0.10",
    "graphql-tools": "^7.0.5",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "postmark": "^2.7.6",
    "sanitize-html": "^2.4.0",
    "slugs": "^0.1.3",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/helmet": "0.0.48",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/morgan": "^1.9.2",
    "@types/node": "^16.0.0",
    "@types/node-cron": "^2.0.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.9",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  }
}
